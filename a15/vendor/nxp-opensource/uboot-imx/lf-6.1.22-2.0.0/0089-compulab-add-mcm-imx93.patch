+int board_ehci_usb_phy_mode(struct udevice *dev)
+{
+	int ret = 0;
+	debug("%s %d\n", __func__, dev_seq(dev));
+
+	if (dev_seq(dev) == 0)
+		ret=USB_INIT_DEVICE;
+	else
