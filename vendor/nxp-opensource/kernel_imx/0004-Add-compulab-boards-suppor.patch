+		};
+		user-led-a-red {
+			label = "Red_A";
+			color = <LED_COLOR_ID_RED>;
+			gpios = <&pca9555_sb 3 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+		user-led-b-green {
+			label = "Green_B";
+			color = <LED_COLOR_ID_GREEN>;
+			gpios = <&pca9555_sb 2 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+		};
+		user-led-b-red {
