+		compatible = "gpio-reset";
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_tpm_reset>;
+		reset-gpios = <&gpio2 12 GPIO_ACTIVE_HIGH>;
+		#reset-cells = <0>;
+		reset-delay-us = <1000>;
+		reset-post-delay-ms = <1>;
+		initially-in-reset;
