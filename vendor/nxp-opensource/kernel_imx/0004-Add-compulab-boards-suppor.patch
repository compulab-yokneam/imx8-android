+
+&flexspi1 {
+	pinctrl-names = "default", "sleep";
+	pinctrl-0 = <&pinctrl_flexspi1>;
+	pinctrl-1 = <&pinctrl_flexspi1>;
+	status = "disabled";
+
+	/* TODO: switch to use reset-gpios */
+	//reset-gpios = <&gpio5 11 GPIO_ACTIVE_LOW>;
+
+	pinctrl-assert-gpios = <&gpio5 11 GPIO_ACTIVE_HIGH>;
+
