+	w1_04: w1-04 {
+		compatible = "w1-gpio";
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_w1_04>;
+		gpios = <&gpio5 5 GPIO_ACTIVE_HIGH>;
