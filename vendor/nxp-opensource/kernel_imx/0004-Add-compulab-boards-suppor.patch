+				snps,dcb-algorithm;
+				snps,priority = <0x8>;
+				snps,map-to-dma-channel = <3>;
+			};
+			queue4 {
+				snps,dcb-algorithm;
+				snps,priority = <0xf0>;
+				snps,map-to-dma-channel = <4>;
+			};
