From 76c3cc20d26735f0bc97bab80ac90ac8a6aa9b06 Mon Sep 17 00:00:00 2001
From: "Nadav (NG) Greenberg" <NadavG@Sysmop.com>
Date: Tue, 6 Oct 2020 16:54:22 +0300
Subject: [PATCH 36/45] StarTek-Driver-LocationMark

---
 drivers/gpu/drm/panel/panel-startek-kd070hdf.c | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/drivers/gpu/drm/panel/panel-startek-kd070hdf.c b/drivers/gpu/drm/panel/panel-startek-kd070hdf.c
index b8de144bc716..1ceb9cc8a58b 100644
--- a/drivers/gpu/drm/panel/panel-startek-kd070hdf.c
+++ b/drivers/gpu/drm/panel/panel-startek-kd070hdf.c
@@ -109,6 +109,7 @@ static int kd070hdf_panel_prepare(struct drm_panel *panel)
 	struct kd070hdf_panel *ctx = panel_to_kd070hdf_panel(panel);
 	int ret,i;
 
+printk("*****\nLocation: %s\n*****\n", __func__);
 	ret = regulator_enable(ctx->supply);
 	if (ret < 0) {
 		DRM_DEV_ERROR(&ctx->dsi->dev, "Failed to enable supply: %d\n", ret);
@@ -152,6 +153,7 @@ static int kd070hdf_panel_enable(struct drm_panel *panel)
 	struct kd070hdf_panel *ctx = panel_to_kd070hdf_panel(panel);
 	int ret;
 
+printk("*****\nLocation: %s\n*****\n", __func__);
 	ret = mipi_dsi_dcs_exit_sleep_mode(ctx->dsi);
 	if (ret)
 		return ret;
@@ -202,6 +204,7 @@ static int kd070hdf_panel_get_modes(struct drm_panel *panel)
 	struct drm_display_mode *mode;
 	static const u32 bus_format = MEDIA_BUS_FMT_RGB888_1X24;
 
+printk("*****\nLocation: %s\n*****\n", __func__);
 	mode = drm_mode_duplicate(panel->drm, &default_mode);
 	if (!mode) {
 		DRM_DEV_ERROR(&ctx->dsi->dev,
@@ -238,6 +241,7 @@ static int kd070hdf_panel_dsi_probe(struct mipi_dsi_device *dsi)
 	struct device_node *np;
 	int ret;
 
+printk("*****\nLocation: %s\n*****\n", __func__);
 	ctx = devm_kzalloc(&dsi->dev, sizeof(*ctx), GFP_KERNEL);
 	if (!ctx)
 		return -ENOMEM;
